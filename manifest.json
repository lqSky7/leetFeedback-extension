{
    "manifest_version": 3,
    "name": "Traverse",
    "version": "1.3.9",
    "description": "Chrome extension for Traverse Ecosystem",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "permissions": [
        "storage",
        "scripting",
        "activeTab",
        "tabs",
        "sidePanel"
    ],
    "host_permissions": [
        "https://*.leetcode.com/*",
        "https://*.geeksforgeeks.org/*",
        "https://practice.geeksforgeeks.org/*",
        "https://*.takeuforward.org/*",
        "https://api.github.com/*",
        "https://generativelanguage.googleapis.com/*",
        "https://traverse-backend-api.azurewebsites.net/*",
        "https://leet-feedback.vercel.app/*",
        "https://*.vercel.app/*",
        "https://*.netlify.app/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.leetcode.com/*"
            ],
            "js": [
                "utils/common.js",
                "utils/github-api.js",
                "utils/gemini-api.js",
                "utils/backend-api.js",
                "utils/problem-timer.js",
                "utils/toast.js",
                "content-scripts/leetcode.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "https://*.geeksforgeeks.org/*",
                "https://practice.geeksforgeeks.org/*"
            ],
            "js": [
                "utils/common.js",
                "utils/github-api.js",
                "utils/gemini-api.js",
                "utils/backend-api.js",
                "utils/problem-timer.js",
                "utils/toast.js",
                "content-scripts/geeksforgeeks.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "https://*.takeuforward.org/*"
            ],
            "js": [
                "utils/common.js",
                "utils/github-api.js",
                "utils/gemini-api.js",
                "utils/backend-api.js",
                "utils/problem-timer.js",
                "utils/toast.js",
                "content-scripts/takeuforward.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "https://leet-feedback.vercel.app/*",
                "https://*.vercel.app/*",
                "https://*.netlify.app/*"
            ],
            "js": [
                "content-scripts/website-auth.js"
            ],
            "run_at": "document_end"
        }
    ],
    "action": {
        "default_title": "Open LeetFeedback",
        "default_icon": {
            "16": "icons/16.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
        }
    },
    "side_panel": {
        "default_path": "sidepanel/sidepanel.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "utils/interceptor.js",
                "utils/monaco-bridge.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}